  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "cover": "react-scripts test --coverage --no-cache --watchAll=false --maxWorkers=8",
    "tdd": "react-scripts test",
    "test": "cross-env CI=true react-scripts test --no-cache --maxWorkers=8",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/http/**/*",
      "!src/mock/**/*",
      "!src/store/**/*",
      "!src/**/store/**/index.{js,ts}",
      "!src/index.tsx",
      "!src/App.tsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
