    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^6.0.0",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^6.0.0",
    "prettier": "^2.3.0",
    "react-beautiful-dnd-test-utils": "^3.2.1",
